{
  "expo": {
    "name": "FadeApp",
    "slug": "FadeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "backgroundColor": "#000030",
    "userInterfaceStyle": "dark",
    "androidNavigationBar": {
      "barStyle": "light-content",
      "visible": "immersive",
      "backgroundColor": "#000030"
    }
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "icon": "./assets/images/app-icon.png",
      "backgroundColor": "#000030"
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to set reminders."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBBvc0PY-q9bEQIxlAPzmv_wp1RQsfyaLk",
          
        }
      },
      "package": "com.oscarlunadotdev.FadeApp",
      "permissions": [
        "READ_CALENDAR", "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "luna-reactdev.auth0.com"
        }
      ]
    ]
  }
