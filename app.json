{
  "expo": {
    "name": "FadeApp",
    "slug": "FadeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "backgroundColor": "#000030",
    "userInterfaceStyle": "dark",

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "icon": "./assets/images/app-icon.png",
      "backgroundColor": "#000030"
    },
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBBvc0PY-q9bEQIxlAPzmv_wp1RQsfyaLk"
        }
      },
      "package": "com.oscarlunadotdev.FadeApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "luna-reactdev.auth0.com"
        }
      ]
    ]
  }
}
