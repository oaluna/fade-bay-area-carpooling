{
  "expo": {
    "name": "FadeApp",
    "slug": "FadeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon/favicon.png",
    "backgroundColor": "#000030",
    "userInterfaceStyle": "dark",
    "androidNavigationBar": {
      "barStyle": "light-content",
      "visible": "sticky-immersive",
      "backgroundColor": "#000030"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "icon": "./assets/app-icon/favicon.png",
      "backgroundColor": "#000030"
    },
    "android": {
      "package": "com.oscarlunadotdev.FadeApp"
    }
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true,
    "infoPlist": {
      "NSCalendarsUsageDescription": "This app needs access to your calendar to set reminders."
    },
    "bundleIdentifier": "com.oscarlunadotdev.FadeApp"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/app-icon/splash.png",
      "backgroundColor": "#000030"
    },
    "config": {
      "googleMaps": {
        "apiKey": "AIzaSyBBvc0PY-q9bEQIxlAPzmv_wp1RQsfyaLk"
      }
    },
    "package": "com.oscarlunadotdev.FadeApp",
    "permissions": [
      "READ_CALENDAR",
      "WRITE_CALENDAR"
    ]
  },
  "web": {
    "favicon": "./assets/app-icon/favicon.png"
  },
  "plugins": [
    [
      "react-native-auth0",
      {
        "domain": "luna-reactdev.auth0.com"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["react-native-paper/babel"],
    },
  },
}
